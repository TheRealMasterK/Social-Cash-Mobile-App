{
  "expo": {
    "name": "banterbubblesnative",
    "slug": "banterbubblesnative",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "owner": "cryptobanter",
    "scheme": "myapp", // TODO: replace with your own app scheme - https://docs.expo.dev/routing/installation/#modify-project-configuration
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.cryptobanter.banterbubblesnative"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.cryptobanter.banterbubblesnative"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "22363afd-b409-4a12-a4f9-54bfc61a704f"
      }
    },
    "plugins": [
      "expo-router"
    ]
  }
}
